    "@babel/runtime": "^7.7.7",
    "@mdi/font": "^4.7.95",
    "@vue/cli-service": "^4.1.2",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "deepmerge": "^4.2.0",
    "fibers": "^4.0.1",
    "html-webpack-plugin": "^3.2.0",
    "material-design-icons-iconfont": "^5.0.1",
    "sass": "^1.24.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
